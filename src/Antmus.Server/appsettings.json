{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Information"
      },
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "http://localhost:8080", /* list of origins split by ';' (without whitespace) */
  "Mode": "Mock", /* Recorder, Mock */
  "Redirect": "http://localhost:8080",
  "RequestHeaders": [ "Cookie" ],
  "ResponseHeaders": [],
  "Mocks": {
    "Path": "mocks"
  }
}